include "WistMath.dll"

func Start() {
    // WhileLoop()
    ForLoop()
}

func ForLoop() {
    for c = 0; c < 10; c = c + 1 {
        if c < 2: continue
    
        Print(c)
        
        if c > 6: break
    }
}

func WhileLoop() {
    c = 0
    while true {
        Print(c) 
        c = c + 1
        if c != 5: continue
        
        Print("c is five!!!")
        if c == 5: break
    }
}